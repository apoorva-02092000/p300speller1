		$(document).ready(function(){
			if(sessionStorage.getItem("firstProtocol") == "true"){
				document.getElementById("start1").style.visibility = "visible";
			}else if(sessionStorage.getItem("secondProtocol") == "true"){
				document.getElementById("start2").style.visibility = "visible";
			}else if(sessionStorage.getItem("thirdProtocol") == "true"){
				document.getElementById("start3").style.visibility = "visible";
			}else if(sessionStorage.getItem("fourthProtocol") == "true"){
				document.getElementById("start4").style.visibility = "visible";
			}else if(sessionStorage.getItem("fifthProtocol") == "true"){
				document.getElementById("start5").style.visibility = "visible";
			}else if(sessionStorage.getItem("sixthProtocol") == "true"){
				document.getElementById("start6").style.visibility = "visible";
			}else if(sessionStorage.getItem("seventhProtocol") == "true"){
				document.getElementById("start7").style.visibility = "visible";
			}else if(sessionStorage.getItem("eighthProtocol") == "true"){
				console.log("yo")
				document.getElementById("start8").style.visibility = "visible";
			}
		})
</script>

