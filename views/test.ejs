<body class="container mt-3" >

<div class="row">
	<div class="col-md-6 col-sm-12 mb-3"  id="speller_matrix">
		<button id="start2" class="btn btn-outline-primary dis" style="visibility: hidden">Start</button>

<table style="margin-left:250px;border-collapse: separate;border-spacing: 30px 10px;
">
		<colgroup>
			<col class="a7">
			<col class="a8">

		</colgroup>
		<tr class="a1">
			<td class="5 1 11" id="A" style="margin: 10px;">Red</td>
			<td class="6 1 12" id="B">Blue</td> 
		</tr>
  
		<tr class="a2">
			<td class="5 2 17" id="G">Circle</td>
			<td class="6 2 18" id="H">Square</td> 
		</tr>
	

	</table>
	
	</div>

	</div>

    <p class="mt-2">Start Time:<span id="time"></span></p>
	<p class="d-none ">First Stimulus:<span id="f_s"></span></p>
	<button  style="margin-top: 10px;" class="btn btn-outline-success mt-1" id="showData">Show selected numbers</button>
	<button  style="margin-top: 10px;" class="btn btn-success mt-1 dis" id="showTime">Show time of stimuli</button>
	<div class="numbers">
	<p id="data" class="mt showMe"></p>
	</div>
	<div class="numbers">
	

		<canvas id ="draw" width = "200" height = "200"></canvas>
	
	
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

	<script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
    

    <script src="trial.js"></script>
	<script src="download.js"></script>
